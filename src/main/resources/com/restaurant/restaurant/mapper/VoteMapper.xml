<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.restaurant.restaurant.mapper.VoteMapper">
    <select id="selectAll" resultType="com.restaurant.restaurant.mapper.VoteMapper">
        SELECT * FROM vote;
    </select>
    <select id="selectById" resultType="com.restaurant.restaurant.mapper.VoteMapper">
        SELECT * FROM vote WHERE vote_id=#{id};
    </select>
</mapper>
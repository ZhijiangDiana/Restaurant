<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.restaurant.restaurant.mapper.MessageMapper">
    <select id="selectAll" resultType="com.restaurant.restaurant.mapper.MessageMapper">
        SELECT * FROM message;
    </select>
    <select id="selectById" resultType="com.restaurant.restaurant.mapper.MessageMapper">
        SELECT * FROM message WHERE message_id=#{id};
    </select>
</mapper>